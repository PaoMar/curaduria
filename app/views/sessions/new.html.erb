 <h1>LOG IN</h1>
  <%= form_tag sessions_path do %>
	<p>
    <%= label_tag :name %><br/>
    <%= text_field_tag :name, params[:name] %>
    <br><br>
    <%= label_tag :password %><br />
    <%= password_field_tag :password, params[:password] %>
  </p>
  <p class="button"><%= submit_tag %> </p>
  <% end %>