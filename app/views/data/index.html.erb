<div id="user_nav">
    <% if current_user %>
    logged in as <%= current_user.name %>
    <%= link_to "log out" , log_out_path, "data-no-turbolink"  => true%>
    <% else %>
     <!--  <%= link_to "Sign up" , sign_up_path%> -->
      <%= link_to "log in" , log_in_path, "data-no-turbolink"  => true%>
    <% end %>
  </div>

<h1>Listing data</h1>

<table>
  <thead>
    <tr>
      <th>Definicion</th>
      <th>Anio</th>
      <th>Siglas</th>
      <th>Valor</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |datum| %>
      <tr>
        <td><%= datum.definicion %></td>
        <td><%= datum.anio %></td>
        <td><%= datum.siglas %></td>
        <td><%= datum.valor %></td>
        <td><%= link_to 'Show', datum %></td>
        <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
        <td><%= link_to 'Destroy', datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datum', new_datum_path %>
